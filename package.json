    "@ngneat/spectator": "6.1.2",
    "@nrwl/cli": "11.0.18",
    "@nrwl/cypress": "11.0.18",
    "@nrwl/eslint-plugin-nx": "11.0.18",
    "@nrwl/jest": "11.0.18",
