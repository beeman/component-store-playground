    "firebase": "8.2.1",
    "immer": "8.0.0",
    "lodash.clonedeep": "4.5.0",
    "ng-indexed-db": "0.0.0-beta.1",
    "ngrx-immer": "0.0.4",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
